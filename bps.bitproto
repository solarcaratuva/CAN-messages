proto bps

option c.name_prefix = "Bitproto"

message BPSPackInformation {
    uint16 pack_voltage = 1;
    uint16 pack_current = 2;
    uint8 pack_soc = 3;
    bool discharge_relay = 4;
    bool charge_relay = 5;
}

message BPSError {
    bool internal_communications_fault = 1;
    bool internal_conversion_fault = 2;
    bool weak_cell_fault = 3;
    bool low_cell_voltage_fault = 4;
    bool open_wiring_fault = 5;
    bool current_sensor_fault = 6;
    bool pack_voltage_sensor_fault = 7;
    bool weak_pack_fault = 8;
    bool voltage_redundancy_fault = 9;
    bool fan_monitor_fault = 10;
    bool thermistor_fault = 11;
    bool CANBUS_communications_fault = 12;
    bool always_on_supply_fault = 13;
    bool high_voltage_isolation_fault = 14;
    bool power_supply_12v_fault = 15;
    bool charge_limit_enforcement_fault = 16;
    bool discharge_limit_enforcement_fault = 17;
    bool charger_safety_relay_fault = 18;
    bool internal_memory_fault = 19;
    bool internal_thermistor_fault = 20;
    bool internal_logic_fault = 21;
}

message BPSCellVoltage {
    uint16 low_cell_voltage = 1;
    uint8 low_cell_voltage_id = 2;
    uint16 high_cell_voltage = 3;
    uint8 high_cell_voltage_id = 4;
}

message BPSCellTemperature {
    uint8 low_temperature = 1;
    uint8 low_thermistor_id = 2;
    uint8 high_temperature = 3;
    uint8 high_thermistor_id = 4;
}
